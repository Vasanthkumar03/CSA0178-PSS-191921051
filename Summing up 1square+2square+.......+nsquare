#include <stdio.h>
int main() 
{
    int n, sum;
    printf("Enter the value of n: ");
    scanf("%d", &n);
    if (n < 0) 
{
        printf("Please enter a non-negative value of n.\n");
        return 1;
}
    sum = (n * (n + 1) * (2 * n + 1)) / 6;
    printf("Sum of the series 1^2 + 2^2 + ... + %d^2 is: %d\n", n, sum);
    return 0;
}
